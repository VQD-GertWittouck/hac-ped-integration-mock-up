<xs:schema targetNamespace="http://www.nrf-arts.org/IXRetail/namespace" xmlns="http://www.nrf-arts.org/IXRetail/namespace" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:include schemaLocation="IFSF_DR_UnitOfMeasureCode.xsd"/>
	<xs:include schemaLocation="DR_BasicTypes.xsd"/>
	<xs:annotation>
		<xs:documentation>Common Types definitions for IFSF POS / EPS Interface  - version 1.0</xs:documentation>
	</xs:annotation>
	<xs:simpleType name="CardPANType">
		<xs:annotation>
			<xs:documentation> Generic Card PAN</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="9"/>
			<xs:maxLength value="28"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CardTrackByteType">
		<xs:annotation>
			<xs:documentation>GenericCardTrack image (1, 2, or 3 Ref. ISO) byte</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:hexBinary"/>
	</xs:simpleType>
	<xs:complexType name="CardTrack">
		<xs:annotation>
			<xs:documentation>GenericCardTrack image (1, 2, or 3 Ref. ISO)</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="Byte" type="CardTrackByteType" minOccurs="0" maxOccurs="104"/>
			<xs:element name="Ascii" minOccurs="0" maxOccurs="104"/>
		</xs:choice>
	</xs:complexType>
